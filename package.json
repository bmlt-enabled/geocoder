		"@sveltejs/vite-plugin-svelte": "^6.0.0-next.2",
		"@types/eslint": "^9.0.0",
		"@types/google.maps": "^3.55.9",
		"@types/leaflet": "^1.9.8",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^10.0.0",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"leaflet": "^1.9.4",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tslib": "^2.6.3",
		"typedoc": "^0.28.4",
